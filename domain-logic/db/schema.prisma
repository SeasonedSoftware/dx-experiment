generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model task {
  id         String   @id @default(dbgenerated("gen_random_uuid()")) @db.Uuid
  text       String
  completed  Boolean  @default(false)
  created_at DateTime @unique @default(now()) @db.Timestamp(6)
}
