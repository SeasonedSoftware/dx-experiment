{
  "name": "ui",
  "version": "6.1.1",
  "private": true,
  "license": "ISC",
  "prisma": {
    "schema": "../domain-logic/db/schema.prisma"
  },
  "scripts": {
    "build": "yarn tw --minify && prisma generate && yarn migrate && next build",
    "dev": "yarn tw --watch & yarn dx:next",
    "dx:next": "prisma generate && next dev",
    "dx:prisma-studio": "prisma studio",
    "dx": "run-p dx:* --print-label",
    "lint:types": "tsc --project tsconfig.json --noEmit",
    "migrate-dev": "prisma migrate dev --preview-feature",
    "migrate": "prisma migrate deploy --preview-feature",
    "start": "next start -p $PORT",
    "studio": "prisma studio",
    "tw": "npx tailwindcss -i ./styles/global.css -o ./public/styles.css",
    "test": "jest"
  },
  "dependencies": {
    "@hookform/resolvers": "^2.7.1",
    "axios": "0.21.1",
    "domain-logic": "link:../domain-logic",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^7.12.2",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/react": "17.0.14",
    "babel-plugin-superjson-next": "^0.3.0",
    "jest": "^27.0.6",
    "next-transpile-modules": "^8.0.0",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "tailwindcss": "^2.2.7",
    "ts-jest": "^27.0.4",
    "typescript": "4.3.5"
  },
  "publishConfig": {
    "access": "restricted"
  }
}
